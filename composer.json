{
    "name": "lwilhem/csv-display-tool",
    "description": "Utility Package to Display CSV Files Data in CLI",
    "type": "symfony-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CsvDisplayTool\\CsvParser\\": "FileParser/",
            "CsvDisplayTool\\CsvFormatter\\": "CsvFormatter/",
            "CsvDisplayTool\\Command\\": "Command/"
        }
    },
    "authors": [
        {
            "name": "Wilhem Lecanu",
            "email": "wilhem.lecanu@edu.devinci.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2",
        "symfony/console": "6.0.x-dev",
        "symfony/serializer": "6.0.x-dev",
        "symfony/property-access": "6.0.x-dev"
    }
}
